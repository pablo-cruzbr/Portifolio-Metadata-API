{
  "rewrites": [
    { 
      "source": "/((?!.*\\.(?:apk|jpg|png|svg|json|css|js)$)[^/]*.*)", 
      "destination": "/" 
    }
  ],
  "headers": [
    {
      "source": "/(.*).apk",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/vnd.android.package-archive"
        },
        {
          "key": "Content-Disposition",
          "value": "attachment; filename=\"techos.apk\""
        }
      ]
    }
  ]
}