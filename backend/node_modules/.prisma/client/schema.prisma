generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Project {
  id           String   @id @default(uuid())
  title        String
  technologies String?
  goal         String?
  features     String?
  image_url    String?
  created_at   DateTime @default(now())
  update_at    DateTime @default(now())

  @@map("projects")
}
