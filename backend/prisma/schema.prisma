generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Project {
  id            String   @id @default(uuid())
  title         String
  technologies  String?
  goal          String?
  features      String?
  created_at    DateTime @default(now())
  update_at     DateTime @default(now())

  @@map("projects")
}
